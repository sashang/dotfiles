set imap_user=sashan
set imap_pass=`lpass show --password mx.zenskg.net`
set folder=imaps://mx.zenskg.net/
set spoolfile=+INBOX
mailboxes =INBOX =Sent
set record = +Sent
set from=sashan@zenskg.net
set realname = 'sashan'
set smtp_url=smtp://$imap_user:$imap_pass@mx.zenskg.net:587/
set ssl_starttls=yes
source ~/.mutt/muttrc
